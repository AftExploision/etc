URxvt.scrollBar:          off
URxvt*font:               -benis-uushi-medium-r-normal--11-90-75-75-p-58-iso10646-1
URxvt*boldFont:           -benis-uushi-medium-r-normal--11-90-75-75-p-58-iso10646-1
URxvt*letterSpace:        0
URxvt.internalBorder:     1
URxvt*selectToClipboard:  true
URxvt*saveLines:          10000

URxvt.perl-ext-common:    default,matcher,searchable-scrollback
URxvt.urlLauncher:        /usr/bin/google-chrome-stable
URxvt.matcher.button:     1

! black background and white foreground
